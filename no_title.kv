MDFloatLayout:
	BoxLayout:
		orientation: "vertical"
		size: root.width, root.height
	
		
		Label:
			text_size: self.size
			halign: "center"
			valign: "middle"
			text: "Click the Button To Close The App"
			font_size: 32


		Button:
			size_hint: (1, .5)
			font_size: 32
			text: "Close App"
			on_press: app.stop()
		

			